        },
        {
            "path": "dist/bgs-form.esm.js",
            "limit": "10 KB"
        }
    ],
    "devDependencies": {
        "@babel/core": "^7.24.0",
        "@size-limit/preset-small-lib": "^11.0.2",
        "@storybook/addon-essentials": "^7.6.17",
